{
  "name": "yk-cli",
  "version": "3.1.4",
  "description": "ykcli for ssr",
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --sourceMap false\" \" tsc -p ./tsconfig.es6.json --sourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.es6.json \""
  },
  "bin": {
    "ykcli": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/yk-cli.git"
  },
  "dependencies": {
    "download-git-repo": "^2.0.0",
    "inquirer": "6.3.1",
    "ora": "^3.4.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "shelljs": "^0.8.3",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/inquirer": "^6.0.3",
    "@types/shelljs": "^0.8.5",
    "@types/webpack": "^4.39.3",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/yargs": "^13.0.0",
    "concurrently": "^5.0.0",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.7.2"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/ykfe/yk-cli/issues"
